{
	"general": {
		/* Shows the rarity of an item in its tooltip.
		   (default: true)
		*/
		"rarityTooltip": false,
		/* Shows the category of an item in a badge on its tooltip.
		   (default: true)
		*/
		"itemBadges": false,
		/* Adjusts the size of tooltips.
		   A scale of 100% displays the tooltip at its default size.
		   (default: 1.0)
		*/
		"scaleFactor": 1.0
	},
	"popUpAnimation": {
		/* Toggles the tooltip pop-up animation.
		   (default: true)
		*/
		"enabled": false,
		/* Duration of the pop-up animation in seconds.
		   (default: 1.5)
		*/
		"time": 1.5,
		/* Magnitude of the pop-up animation.
		   (default: 1.0)
		*/
		"magnitude": 1.0
	},
	"itemPreviewAnimation": {
		/* Toggles the item preview bouncing animation.
		   (default: true)
		*/
		"enabled": true,
		/* Duration of the item preview animation in seconds.
		   (default: 1.0)
		*/
		"time": 2.0,
		/* Magnitude of the item preview animation.
		   (default: 2.0)
		*/
		"magnitude": 0.5
	},
	"border": {
		/* Determines how the border color of tooltips is set.
		   RARITY/ITEM_NAME/CUSTOM (default: RARITY)
		*/
		"borderColor": "CUSTOM",
		// Custom border colors when borderColor is set to CUSTOM.
		"customBorderColors": {
			/* 'Common' rarity border color.
			   (default: #5000FF50)
			*/
			"common": "#00000000",
			/* 'Uncommon' rarity border color.
			   (default: #FFFF55FF)
			*/
			"uncommon": "#00000000",
			/* 'Rare' rarity border color.
			   (default: #55FFFFFF)
			*/
			"rare": "#00000000",
			/* 'Epic' rarity border color.
			   (default: #FF00FFFF)
			*/
			"epic": "#00000000",
			/* Gradient end color for the border.
			   (default: #5000FF50)
			*/
			"endColor": "#00000000"
		}
	},
	"background": {
		/* Background color of the tooltip.
		   (default: #100010F0)
		*/
		"backgroundColor": "#222222FA"
	},
	"foodAndDrinks": {
		/* Shows the maximum hunger which can be gained from an item in its tooltip.
		   (default: true)
		*/
		"hungerTooltip": true,
		/* Shows the maximum saturation which can be gained from an item in its tooltip.
		   (default: true)
		*/
		"saturationTooltip": true,
		/* Shows a list of effects applied on consuming an item in its tooltip.
		   OFF/WITHOUT_ICONS/WITH_ICONS (default: WITH_ICONS)
		*/
		"effectsTooltip": "WITHOUT_ICONS"
	},
	"mobs": {
		/* The rotation speed of the model.
		   (default: 0.2)
		*/
		"rotationSpeed": 0.05000000074505806,
		/* Shows a preview of the armor piece on an armor stand.
		   (default: true)
		*/
		"armorTooltip": true,
		/* Shows a preview of the horse armor on a horse.
		   (default: true)
		*/
		"horseArmorTooltip": false,
		/* Shows a preview of the wolf armor on a wolf.
		   (default: true)
		*/
		"wolfArmorTooltip": false,
		/* Shows a preview of the bucket entity in a bucket.
		   (default: true)
		*/
		"bucketTooltip": false,
		/* Shows a preview of the spawn egg entity.
		   (default: true)
		*/
		"spawnEggTooltip": false
	},
	"mapTooltip": {
		/* Shows a preview of the filled map in its tooltip.
		   (default: true)
		*/
		"enabled": true
	},
	"paintingTooltip": {
		/* Shows a preview of the painting in its tooltip.
		   (default: true)
		*/
		"enabled": false
	},
	"durability": {
		/* Shows the durability of an item in its tooltip.
		   OFF/VALUE/PERCENTAGE (default: VALUE)
		*/
		"durabilityTooltip": "VALUE",
		/* Shows the durability of an item, represented by a bar, in its tooltip.
		   (default: false)
		*/
		"durabilityBar": false
	}
}